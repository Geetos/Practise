                        <th>操作</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr th:if="${QX.query == '1'}" th:each="alterschedule,stat :${alterschedules}">
                        <td th:text="${stat.count}">1</td>
                        <td th:text="${alterschedule.content}"></td>
                        <span th:if="${alterschedule.alterdate!=null}"><td th:text="${#dates.format(alterschedule.alterdate,'yyyy-MM-dd')}"></td></span>
                        <span th:unless="${alterschedule.alterdate!=null}"><td ></td></span>


                        <td th:text="${alterschedule.altertime}"></td>
                        <td th:text="${alterschedule.lasttotaldays}"></td>
                        <td th:text="${alterschedule.lastcontractMoney}"></td>

                        <span th:if="${alterschedule.laststartdate!=null}"><td th:text="${#dates.format(alterschedule.laststartdate,'yyyy-MM-dd')}"></td></span>
                        <span th:unless="${alterschedule.laststartdate!=null}"><td ></td></span>

                        <span th:if="${alterschedule.lastenddate!=null}"><td th:text="${#dates.format(alterschedule.lastenddate,'yyyy-MM-dd')}"></td></span>
                        <span th:unless="${alterschedule.lastenddate!=null}"><td ></td></span>

                        <span th:if="${alterschedule.lastinitialdate!=null}"><td th:text="${#dates.format(alterschedule.lastinitialdate,'yyyy-MM-dd')}"></td></span>
                        <span th:unless="${alterschedule.lastinitialdate!=null}"><td></td></span>

