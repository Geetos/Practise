                        <span th:unless="${alterschedule.lastfinaldate!=null}"><td ></td></span>

                    <!--    <td th:text="${#dates.format(alterschedule.laststartdate,'yyyy-MM-dd')}"></td>
                        <td th:text="${#dates.format(alterschedule.lastenddate,'yyyy-MM-dd')}"></td>
                        <td th:text="${#dates.format(alterschedule.lastinitialdate,'yyyy-MM-dd')}"></td>
                        <td th:text="${#dates.format(alterschedule.lastfinaldate,'yyyy-MM-dd')}"></td>-->
                        <td >
                            <span data-toggle="tooltip" title="编辑进度信息" th:if="${QX.edit == '1'}" class="btn btn-xs btn-info"
                                  th:onclick="'editAlterschedule(\''+${alterschedule.id}+'\'
                                  ,\''+${alterschedule.ctScheduleid}+'\'
                                  ,\''+${alterschedule.content}+'\'
                                  ,\''+${alterschedule.alterdate}+'\'
                                  ,\''+${alterschedule.altertime}+'\'
                                  ,\''+${alterschedule.lasttotaldays}+'\'
                                  ,\''+${alterschedule.laststartdate}+'\'
                                  ,\''+${alterschedule.lastenddate}+'\'
                                  ,\''+${alterschedule.lastinitialdate}+'\'
                                  ,\''+${alterschedule.lastfinaldate}+'\'
                                  ,\''+${alterschedule.lastwarrantyClaim}+'\'
                                  ,\''+${alterschedule.projectId}+'\'
                                   ,\''+${alterschedule.moneyinfoId}+'\'
                                  ,\''+${alterschedule.lastcontractMoney}+'\'
                                  ,\''+${alterschedule.lastcostMoney}+'\'
                                  ,\''+${alterschedule.lastsubcontracting}+'\'
                                  ,\''+${alterschedule.lastprofitMargin}+'\'
                                  ,\''+${alterschedule.alertsec}+'\'
                                  ,\''+${alterschedule.alertmon}+'\'
                                  ,\''+${alterschedule.alertstatus}+'\'
                                  );'">
                                <i class="fa fa-edit"></i>
                            </span>

                            <span data-toggle="tooltip" title="附件" th:if="${QX.del == '1'}"
                                  class="btn btn-info"
                                  th:onclick="'findfile(\''+${alterschedule.id}+'\');'"><i
                                    class="fa fa-trash-o"></i>附件 </span>

                            <button th:if="${alterschedule.alertsec == 1}" class="btn btn-success btn-sm" id="secModalchange"  th:onclick="'secModalchange(\''+${alterschedule.id}+'\' ,\''+${alterschedule.projectId}+'\');'"><i
                                    class="fa fa-plus"></i> &nbsp;&nbsp;进度变更
                            </button>

                            <button th:if="${alterschedule.alertmon == 1}" class="btn btn-success btn-sm" id="monModalchange"  th:onclick="'monModalchange(\''+${alterschedule.id}+'\', \''+${alterschedule.projectId}+'\');'"><i
