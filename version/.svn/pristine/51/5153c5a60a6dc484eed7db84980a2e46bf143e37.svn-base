    //验证数据
    function checkAccount(username, nickName, password) {
        if (username == '') {
            $("input[name='username']").focus();
            alert("用户名不能为空");
            return false;
        }
        if (nickName == '') {
            $("input[name='nick_name']").focus();
            alert("昵称不能为空");
            return false;
        }
        if (password == '') {
            $("input[name='password']").focus();
            alert("密码不能为空");
            return false;
        } else if (password.length < 4) {
            $("input[name='password']").focus();
            alert("密码个数不能少于4位");
            return false;
        }
        return true;
    }

