    //检查项静态数据列表
    public static List<FileCheckItem> getItems(Integer fid){
        List<FileCheckItem> fileCheckItems = new ArrayList<FileCheckItem>();
        for(int row=0;row<value.length;row++){
//            for(int col=0;col<value[row].length;col++){
            FileCheckItem checkItem = new FileCheckItem();
            String checkName = value[row][0];
            String filetype = value[row][1];
            checkItem.setCheckName(checkName);
            checkItem.setFiletype(filetype);
            checkItem.setFileId(fid);
            fileCheckItems.add(checkItem);
//            }
        }
        return fileCheckItems;
    }
